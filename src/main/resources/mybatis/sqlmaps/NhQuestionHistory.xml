<select id="showNhQhistory" resultMap="NhQHistoryMap">
      SELECT * FROM nh_question_history
   </select>
  <select id="searchHistoryByVersion" resultMap="NhQHistoryMap">
      SELECT * FROM nh_question_history WHERE (Version = #{idx})
   </select>
<select id="getMaxVersion" resultType="String">
      SELECT MAX(CAST(Version AS DECIMAL)) FROM nh_question_history
</select>   
   
<insert id="insertNhQHist" parameterType="com.sanhak.hrsurvey.domain.NhQuestionHistory">
      INSERT INTO nh_question_history VALUES(#{ModDate}, #{Version}, #{KR_Q_1}, #{KR_Q_2}, #{KR_Q_3}, #{KR_Q_4}, #{KR_Q_5}, #{KR_Q_6}, #{KR_Q_7}, #{KR_Q_8}, #{KR_Q_9}, #{KR_Q_10}, #{KR_Q_11}, #{KR_Q_12}, #{KR_Q_13}, #{KR_Q_14}, #{KR_Q_15}, #{EN_Q_1}, #{EN_Q_2}, #{EN_Q_3}, #{EN_Q_4}, #{EN_Q_5}, #{EN_Q_6}, #{EN_Q_7}, #{EN_Q_8}, #{EN_Q_9}, #{EN_Q_10}, #{EN_Q_11}, #{EN_Q_12}, #{EN_Q_13}, #{EN_Q_14}, #{EN_Q_15})
